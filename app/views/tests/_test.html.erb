<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.size %></td>

  <td><%= link_to "Вопросы", test_path(test) %></td>
  <td><%= link_to "Изменить", edit_test_path(test) %></td>  
  <td><%= link_to "Удалить", test_path(test), 
          method: :delete, 
          data: { confirm: "Действительно удалить?" } %>
  </td>
   <td><%= button_to 'Start', start_test_path(test.id) %></td>
</tr>
